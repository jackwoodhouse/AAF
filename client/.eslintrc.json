{
    "root": true,
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "extends": [
        "plugin:vue/essential",
        "@vue/airbnb"
    ],
    "plugins": [
        "vue"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "indent": [
            "error",
            4
        ],
        "no-console": "off"
    },
    "settings": {
        "html/html-extensions": [
            ".html"
        ]
    }
}